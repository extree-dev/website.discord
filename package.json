{
  "name": "website.discord",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently --kill-others-on-fail=false --raw -n SERVER,CLIENT -c \"blue,green\" \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "cd src/server && ts-node index.ts",
    "db:sync": "powershell -Command \"$name=$env:npm_config_name; if (-not $name) { $name=(Get-Date -Format yyyyMMddHHmmss) }; npx prisma migrate dev --name $name; npx prisma generate\""
  },
  "dependencies": {
    "@prisma/client": "^6.14.0",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "dotenv": "^17.2.1",
    "framer-motion": "^12.23.12",
    "gsap": "^3.13.0",
    "lucide-react": "^0.539.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.7.1",
    "recharts": "^3.1.2",
    "tree": "^0.1.3",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "concurrently": "^9.2.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "prisma": "^6.14.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.6"
  }
}
